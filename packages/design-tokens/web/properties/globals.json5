                                foreground: { value: '{light-color-scheme.states.foreground.disabled}' },
                                background: { value: 'transparent' },
                                border: { value: 'transparent' },
                            }
                        }
                    }
                }
            }
        },
        'dark-color-scheme': {}
    }
}
